{
  "name": "mevn-boilerplate",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "heroku-postbuild": "yarn client:build",
    "client:build": "cd client && yarn install && yarn build",
    "dev-server": "NODE_ENV=development nodemon --inspect ./bin/www",
    "client": "concurrently --kill-others-on-fail 'cd client && yarn serve'",
    "dev-start": "concurrently --kill-others-on-fail \"yarn dev-server\" \"yarn client\""
  },
  "dependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.3",
    "@doweb/vuexpress": "^1.1.3",
    "autoprefixer": "^9.8.5",
    "aws-sdk": "^2.853.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "cloudinary": "^1.23.0",
    "concurrently": "^5.2.0",
    "config": "^3.3.1",
    "connect-history-api-fallback": "^1.6.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-enforces-ssl": "^1.1.0",
    "express-jwt": "^6.0.0",
    "express-session": "^1.17.1",
    "helmet": "^3.23.3",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "mailgun-js": "^0.22.0",
    "module-alias": "^2.2.2",
    "mongodb": "^3.5.9",
    "mongoose": "^5.9.20",
    "mongoose-unique-validator": "^2.0.3",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "multer-storage-cloudinary": "^4.0.0",
    "passport": "^0.4.1",
    "passport-auth0": "^1.3.3",
    "passport-local": "^1.0.0",
    "querystring": "^0.2.0",
    "serve-static": "^1.14.1",
    "stripe": "^8.120.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  },
  "_moduleAliases": {
    "@": "."
  }
}
